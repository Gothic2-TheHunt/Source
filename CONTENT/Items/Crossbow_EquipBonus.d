
func void equip_crossbow_01()
{
	if(Npc_IsPlayer(self))
	{
		B_AddFightSkill(self,NPC_TALENT_CROSSBOW,Waffenbonus_01);
	};
};

func void unequip_crossbow_01()
{
	if(Npc_IsPlayer(self))
	{
		B_AddFightSkill(self,NPC_TALENT_CROSSBOW,-Waffenbonus_01);
	};
};

