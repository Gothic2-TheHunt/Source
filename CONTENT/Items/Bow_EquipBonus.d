
func void equip_bow_01()
{
	if(Npc_IsPlayer(self))
	{
		B_AddFightSkill(self,NPC_TALENT_BOW,Waffenbonus_01);
	};
};

func void unequip_bow_01()
{
	if(Npc_IsPlayer(self))
	{
		B_AddFightSkill(self,NPC_TALENT_BOW,-Waffenbonus_01);
	};
};

